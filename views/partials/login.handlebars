  <div id="modal1" class="modal">
    <div class="modal-content">
      <div class="input-field center-align">
        <input placeholder="Email" id="emailSignUp" type="email" class="validate">
        <label for="email"></label>
        <input placeholder="User Name" id="user_name" type="text" class="validate">
        <label for="user_name"></label>
        <input placeholder="Password" id="passwordSignUp" type="password" class="validate">
        <label for="password"></label>
      </div>  
      <div id="Options">
        <label>
          <input name="group1" type="radio" checked />
          <span>Admin</span>
        </label>
        <label>
          <input name="group1" type="radio" checked />
          <span>Standard</span>
        </label>
      </div>
      <div class="modal-footer">
        <button id="signup" class="modal-close waves-effect waves-green btn-flat">Sign Up!</button>
      </div>
    </div>
  </div>
  <script src="/js/login.js"></script>
